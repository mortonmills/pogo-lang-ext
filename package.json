{
  "name": "pogo",
  "displayName": "pogo",
  "description": "Pogo is an experimental markdown language for screenplays.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pogo",
      "aliases": ["pogo", "pogo"],
      "extensions": [".pogo"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pogo",
      "scopeName": "source.pogo",
      "path": "./syntaxes/pogo.tmLanguage.json"
    }]
  }
}
